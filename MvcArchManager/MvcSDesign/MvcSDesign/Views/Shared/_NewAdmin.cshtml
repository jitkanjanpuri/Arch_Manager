        function getSale() {
            var cc = 0;
            var lblArr = [];
            var dataArr = [];

            $.ajax({
                url: "/adminReport/getSaleWidget",
                type: "GET",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (d) {
                    var slist;

                    for (var i = 0; i < d.length; i++) {
                        slist = d[i];
                        lblArr.push(slist.projectType);
                        dataArr.push(slist.sno);
                        cc = cc + parseInt(slist.sno);
                    }
                    document.getElementById("lblSales").innerHTML = cc;

                    var ctx_area_5 = document.getElementById("widgetChart4");
                    ctx_area_5.height = 115;
                    var widgetChart4 = new Chart(ctx_area_5, {
                        type: 'bar',
                        data: {
                            labels: lblArr,
                            datasets: [
                                {
                                    label: "Sales",
                                    data: dataArr,
                                    borderColor: "transparent",
                                    borderWidth: "0",
                                    backgroundColor: "rgba(255,255,255,.3)"
                                }
                            ]
                        },
                        options: {
                            maintainAspectRatio: true,
                            legend: {
                                display: false
                            },
                            scales: {
                                xAxes: [{
                                    display: false,
                                    categoryPercentage: 1,
                                    barPercentage: 0.65
                                }],
                                yAxes: [{
                                    display: false
                                }]
                            }
                        }
                    });

                },
                error: function () {
                    alert("Error ");
                }
            });

        }

