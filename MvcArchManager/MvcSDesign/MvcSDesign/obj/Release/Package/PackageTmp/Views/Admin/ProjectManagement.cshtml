 

@{
    ViewBag.Title = "Project Management";
    Layout = "~/Views/Shared/_NewAdmin.cshtml";

}
<!doctype html>
<html lang="en">

<head>
    <script src="~/angularjs/projectmanagement1.js"></script>
    <script type="text/javascript">

        function IsNumber(e) {

            var theEvent = event || window.event;
            var key = theEvent.keyCode || theEvent.which;
            key = String.fromCharCode(key);
            var regex = /[0-9]|\./;

            if (!regex.test(key)) {
                theEvent.preventDefault ? theEvent.preventDefault() : (theEvent.returnValue = false);
            }
        }
    </script>


</head>
@using (@Html.BeginForm("ProjectManagement","Admin",FormMethod.Post))
{
<body>
    <div ng-app="myApp">
        <div ng-controller="myController">
            <div class="page-content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between mt-4">
                                <div class="col-xl-3 col-md-2">
                                    <div class="form-group mb-3 ">
                                        <h4 class="mb-sm-0 font-size-18 mt-3">Project Management </h4>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                      <br />
                    <div class="row">
                        <div class="col-xl-2 col-md-6">
                            <div class="form-group mb-3">
                                <input type="radio" ng-model="opt" value="quotationNo" class="form-check-input ml-1">
                                <label> <span class="ml-4"> Quotation No </span>  </label>
                                <input type="text" ng-model="txtQuotationNo" onkeypress="IsNumber()" class="form-control" placeholder="Quatation No" />
                            </div>
                        </div>
                        <div class="col-xl-2 col-md-6">
                            <div class="form-group mb-3">
                                <input type="radio" ng-model="opt" value="name" class="form-check-input ml-1 ">
                                <label> <span class="ml-4"> Client Name  </span> </label>
                                <input type="text" ng-model="txtName" class="form-control" placeholder="Client Name" />
                            </div>
                        </div>


                        <div class="col-xl-2 col-md-6">
                            <div class="form-group mb-3">
                                <input type="radio" ng-model="opt" value="projectName" class="form-check-input ml-1">
                                <label> <span class="ml-4"> Project name  </span> </label>
                                <input type="text" ng-model="txtProjectName" class="form-control" placeholder="Project name" />
                            </div>
                        </div>
                        <div class="col-xl-1 col-md-6">
                            <label> . </label>
                            <div class="form-group">
                                <button type="button" class="btn btn-info" ng-click="Search()">Search</button>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xl-12 col-md-12 text-left">
                            <label> <span class="text-danger font-size-16 ">{{errRecord}}</span> </label>
                        </div>
                    </div>
                     
                    <div class="text-center" ng-hide="loading">
                        <button type="button" class="btn btn-dark waves-effect waves-light">
                            <i class="bx bx-loader bx-spin font-size-16 align-middle me-2"></i> Loading
                        </button>
                    </div>
                     
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="card">
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table align-middle datatable dt-responsive table-bordered nowrap" style="border: 1px solid black; border-color:black; border-spacing: 0 8px; width: 100%;">
                                            <thead class="bg-info text-white">
                                                <tr>
                                                    <th>Quotation No</th>
                                                    <th>Client name</th>
                                                    <th>Project Name</th>
                                                    <th>Service </th>
                                                    <th>Amount</th>
                                                    <th>Remark</th>
                                                    <th colspan="3" class="text-center">Action </th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr ng-repeat="item in projectquotationlist">
                                                   
                                                    <td>
                                                        <span style=""> {{item.quotationNo}}  </span>
                                                    </td>

                                                    <td>
                                                        <span style=" ">  {{item.clientName}}  </span>
                                                    </td>
                                                    
                                                    <td>
                                                        <span style=""> {{item.projectName}} </span>
                                                    </td>
                                                    <td>
                                                        <span style=""> {{item.service}} </span>
                                                    </td>
                                                    <td>
                                                        <span style=""> {{item.amount}} </span>
                                                    </td>
                                                    <td>
                                                        <span style=""> {{item.remark}}</span>
                                                    </td>
                                                    <td style="width:70px;">
                                                        <button type="button" class="btn btn-primary waves-effect waves-light w-sm" ng-click="PdfOpen(item.quotationID )">
                                                            PDF
                                                        </button>
                                                    </td>
                                                    <td style="width:70px;">
                                                        <div ng-if="item.status =='request'">
                                                            <button type="button" class="btn btn-success waves-effect waves-light w-sm" data-bs-toggle="modal" data-bs-target=".bs-example-modal-center2" ng-click="ShowPanel(item)">
                                                               Confirm
                                                            </button>
                                                        </div>
                                                         <div ng-if="item.status !='request'">
                                                             <span style="">Project already estimated</span>
                                                        </div>
                                                    </td>
                                                    <td style="width:70px;">
                                                       
                                                        <div ng-if="item.status =='request'">
                                                            <button type="button" class="btn btn-danger waves-effect waves-light w-sm" ng-click="ShowDeleteModel( item.quotationID, item.quotationNo)" data-bs-toggle="modal" data-bs-target=".bs-example-modal-center3">
                                                               Delete
                                                            </button>
                                                         </div>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="modal fade bs-example-modal-center2" tabindex="-1" role="dialog" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title">Project Finalize</h5>
                                    
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div>
                                                <form id="pristine-valid-example" novalidate method="post">
                                                    <input type="hidden" />
                                                    <div class="row">

                                                        <div class="col-xl-12 col-md-6">
                                                            <div class="form-group mb-3">
                                                                <label>Quotation No </label>

                                                                <input type="text" ng-model="QuotationNo" readonly class="form-control" placeholder="Quotation No" />
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-12 col-md-6">
                                                            <div class="form-group mb-3">
                                                                <label>Client name	 </label>
                                                                <input type="text" ng-model="clientName" class="form-control" readonly />
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-12 col-md-6">
                                                            <div class="form-group mb-3">
                                                                <label>Amount</label>
                                                                <input type="text" ng-model="amount" class="form-control" placeholder="  " readonly />
                                                            </div>
                                                        </div>
                                                        <div class="col-xl-12 col-md-6">
                                                            <div class="form-group mb-3">
                                                                <label>Finalize amount</label>
                                                                <input type="text" ng-model="finalAmount" class="form-control" onkeypress="return IsNumber();" placeholder="Finalize Amount  " />
                                                            </div>
                                                        </div>


                                                    </div>
                                                </form>
                                            </div>

                                        </div>
                                        <div class="col-xl-12 col-md-6">
                                            <div class="text-center" ng-hide="loading">
                                                <button type="button" class="btn btn-dark waves-effect waves-light">
                                                    <i class="bx bx-loader bx-spin font-size-16 align-middle me-2"></i> Loading
                                                </button>
                                            </div>
                                        </div>
                                        

                                        <div class="modal-footer">

                                            <button type="button" class="btn btn-secondary waves-effect waves-light" data-bs-dismiss="modal" aria-label="Close">Close</button>
                                            &nbsp;&nbsp;
                                            <button type="button" ng-click="SaveQuotation()" ng-disabled="loadingMail" class="btn btn-primary waves-effect waves-light">Submit</button>
                                        </div>

                                      </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="modal fade bs-example-modal-preview" tabindex="-1" role="dialog" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title">Quotation preview</h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                </div>
                                <div class="modal-body" style="height:500px;">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div>
                                                <form id="pristine-valid-example" novalidate method="post">
                                                    <div class="text-center" ng-hide="loading">
                                                        <button type="button" class="btn btn-dark waves-effect waves-light">
                                                            <i class="bx bx-loader bx-spin font-size-16 align-middle me-2"></i> Loading
                                                        </button>
                                                    </div>
                                                    <div class="row">
                                                        <embed ng-src={{pdfFilePath}} width="790" height="450" />
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal fade bs-example-modal-center3" tabindex="-1" role="dialog" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered">
                            <div class="modal-content">
                                <div class="modal-body">
                                    <div class="modal-header">
                                        <h3 class="modal-title">Delete Quotation</h3>
                                    </div>
                                    <br /><br />
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <span style="font-size:14pt;">  Are you want to delete quotation No <b>{{quoNo}}</b> ? </span><br><br>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-secondary waves-effect waves-light" data-bs-dismiss="modal" aria-label="Close">Close</button>
                                            &nbsp;&nbsp;
                                            <button type="button" class="btn btn-success waves-effect waves-light" ng-click="QuotationDelete()">Yes</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
}

</html>