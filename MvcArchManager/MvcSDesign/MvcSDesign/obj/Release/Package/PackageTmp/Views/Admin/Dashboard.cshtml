 
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_NewAdmin.cshtml";
}


    <!doctype html>
    <html lang="en">
    <head>
          <meta charset="utf-8">
          <meta http-equiv="X-UA-Compatible" content="IE=edge">
          <meta http-equiv="Content-Language" content="en">
          <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
          <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no" />
          <meta name="description" content="This is an example dashboard created using build-in elements and components.">
          <meta name="msapplication-tap-highlight" content="no">
          <link href="~/assets/css/bootstrap.min.css" rel="stylesheet">
          <link href="~/assets/css/dataTables.bootstrap4.min.css" rel="stylesheet">
          <link href="~/assets/css/font-awesome-all.min.css" rel="stylesheet">
          <link href="~/assets/css/style.css" rel="stylesheet">
        <script src="~/angularjs/adminDashboard1.js"></script>

    </head>

    @using (Html.BeginForm("Dashboard", "Admin", FormMethod.Post, new { name = "frmID" }))
    {
        <body style="background-color:white">
            <div ng-app="myApp">
                <div ng-controller="cntr">
                    <div class="container-fluid mt-3">
                        <div class="app-main__outer">
                            <br /><br />
                            <div class="app-main__inner">
                                <div class="row">
                                     <div class="col-xl-12 col-md-6">
                                        <div class="card card-h-100">
                                            <div class="card-body" style="background-color:white">
                                                <div class="row align-items-center">
                                                    <h4>
                                                        <span style="color:#e15050">{{organizationName}}</span>
                                                    </h4>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xl-4 col-md-6">
                                        <div class="card card-h-100">
                                            <div class="card-body" style="background-color:white">
                                                <div class="row align-items-center">
                                                    <div class="col-6">
                                                         <div>
                                                             <h5>Projects Done <span style="font-size: 8pt">(last 30 days)</span> </h5>
                                                         </div>

                                                        <h4 class="mb-3">
                                                            <span>{{prjDay}}</span>
                                                        </h4>
                                                    </div>
                                                    <div class="col-6">
                                                        <div id="mini-chart2" data-colors='["#5156be"]' class="apex-charts mb-2"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xl-4 col-md-6">
                                        <div class="card card-h-100">
                                            <div class="card-body" style="background-color:white">
                                                <div class="row align-items-center">
                                                    <div class="col-6">
                                                        <div>
                                                             <h5>Sales <span style="font-size: 8pt">(last 30 days)</span> </h5>
                                                         </div>
                                                        <h4 class="mb-3">
                                                            <span>{{sales}}</span>
                                                        </h4>
                                                    </div>
                                                    <div class="col-6">
                                                        <div id="mini-chart3" data-colors='["#5156be"]' class="apex-charts mb-2"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xl-4 col-md-6">
                                        <div class="card card-h-100">
                                            <!-- card body -->
                                            <div class="card-body" style="background-color:white">
                                                <div class="row align-items-center">
                                                    <div class="col-6">
                                                        
                                                        <div>
                                                             <h5>Quotation <span style="font-size: 8pt"> (last 30 days)</span> </h5>
                                                         </div>
                                                        <h4 class="mb-3">
                                                            <span>{{quotation}}</span>
                                                        </h4>
                                                    </div>
                                                    <div class="col-6">
                                                        <div id="mini-chart3" data-colors='["#5156be"]' class="apex-charts mb-2"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-xl-6 col-lg-6 col-md-12 mb-3">

                                        <div class="card card-h-100">
                                            <div class="card-body" style="background-color:white">
                                                <div id="carouselExampleIndicators1" class="carousel slide" data-ride="carousel">
                                                    <div class="carousel-inner">
                                                        <div class="carousel-item active">
                                                             <div>
                                                                 <h5>Operations <span style="font-size: 8pt">(last 30 days)</span> </h5>
                                                             </div>

                                                            <canvas id="chartProjectCount"></canvas>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xl-6 col-lg-6 col-md-12 my-3">
                                        <div class="card card-h-100">
                                            <div class="card-body" style="background-color:white">
                                                <div class="tabs-section">
                                                    <div id="carouselExampleIndicators6" class="carousel slide" data-ride="carousel">
                                                        <div class="carousel-inner">
                                                            <div class="carousel-item active">
                                                                <div>
                                                                    <h5>Top Performers <span style="font-size: 8pt">(last 30 days)</span> </h5>
                                                                 </div>
                                                                  <table class="table table-bordered table-hover table-striped">
                                                                    <tbody  style="text-align:center; vertical-align:central;">
                                                                        <tr ng-repeat="item in topperformerslist" >
                                                                            <td style="width:50px; height:50px;">
                                                                              <img src="~/Images/staffPhotos/{{item.photo}}"   class="rounded-circle" style="width:100%; height:90%"  >
                                                                            </td>
                                                                            <td>{{item.name}}</td>
                                                                            <td>{{item.sno}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                 </table>
                                                            </div>
                                                        </div>
                                                    </div> 
                                                </div>
                                            </div>
                                        </div>
                                      
                                        <div class="card card-h-100">
                                            
                                            <div class="card-body" style="background-color:white">
                                                <div class="tabs-section">
                                                    <div id="carouselExampleIndicators6" class="carousel slide" data-ride="carousel">
                                                        <div class="carousel-inner">
                                                            <div class="carousel-item active">
                                                                <div>
                                                                    <h5> Current client working  </h5>
                                                                 </div>


                                                                <table class="table table-bordered table-hover table-striped">
                                                                    <tbody>
                                                                        <tr ng-repeat="item in currentclientworkingList" style="height:30px">
                                                                            <td>{{item.clientName}}</td>
                                                                             
                                                                        </tr>

                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div> 
                                                </div>
                                            </div>
                                        </div>

                                   </div>

                                    <div class="col-xl-6 col-lg-6 col-md-12 my-3">
                                        <div class="card card-h-100">
                                            <div class="card-body" style="background-color:white">
                                                <div class="tabs-section">
                                                    <div id="carouselExampleIndicators6" class="carousel slide" data-ride="carousel">
                                                        <div class="carousel-inner">
                                                            <div class="carousel-item active">
                                                                <div>
                                                                    <h5>Current Working Task</h5>
                                                                 </div>
                                                                <table class="table table-bordered table-hover table-striped">
                                                                    <tbody>
                                                                        <tr ng-repeat="item in currentclientworkingList" style="height:30px">
                                                                            <td>{{item.category}}</td>
                                                                            <td>{{item.subcategory}}</td>
                                                                            <td>{{item.quotationNo}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div> 
                                                </div>
                                            </div>
                                        </div>
                                   </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <script type="text/javascript" src="~/assets/js/jquery.min.js"></script>
            <script type="text/javascript" src="~/assets/js/bootstrap.bundle.min.js"></script>
            <script type="text/javascript" src="~/assets/js/chart.min.js"></script>
            <script type="text/javascript" src="~/assets/js/circle-progress.min.js"></script>

        </body>
    }
    </html>










